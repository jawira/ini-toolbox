{
  "name": "jawira/php-ini-settings",
  "type": "library",
  "description": "Manage your PHP settings",
  "keywords": [
    "php.ini",
    "ini"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jawira Portugal",
      "homepage": "https://jawira.com/"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "jawira/skeleton": "^1.9",
    "behat/behat": "^3.6",
    "pds/skeleton": "^1.0"
  },
  "suggest": {
    "pds/skeleton": "PHP Package Development Standards"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\PhpIniSettings\\": "src/"
    }
  }
}
