{
  "name": "jawira/ini-toolbox",
  "type": "library",
  "description": "Read/write PHP settings (php.ini) at runtime",
  "keywords": [
    "php.ini",
    "ini",
    "settings",
    "toolbox"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jawira Portugal",
      "homepage": "https://jawira.com/"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "behat/behat": "^3.6"
  },
  "suggest": {
    "pds/skeleton": "PHP Package Development Standards",
    "phing/phing": "PHP build tool"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\IniToolbox\\": "src"
    }
  }
}
